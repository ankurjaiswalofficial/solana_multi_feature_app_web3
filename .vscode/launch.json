{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Next.js: Full Debug",
        "runtimeExecutable": "npm",
        "runtimeArgs": [
          "run",
          "dev"
        ],
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen",
        "outputCapture": "std",
        "skipFiles": [
          "<node_internals>/**"
        ]
      },
      {
        "type": "chrome",
        "request": "launch",
        "name": "Next.js: Chrome Debug",
        "url": "http://localhost:3000",
        "webRoot": "${workspaceFolder}",
        "sourceMaps": true,
        "skipFiles": ["<node_internals>/**"]
      }
    ]
  }
  